package constructor;

public class MyDetails {

	double accNo;
	String name;
	String bankName;
	
	public MyDetails(double an,String n,String bName) {
		accNo=an;
		name=n;
		bankName=bName;
	}
	
	void displayDetails(){
		System.out.println(accNo+" "+name+" "+bankName);
		}
	
	public static void main(String[] args) {
				
		MyDetails m=new MyDetails(059801540737,"Rajkumar","Icici");
	}

}
